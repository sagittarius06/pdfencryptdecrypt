Exec[nl]=width_default=300;title_length_threshold=20;short_width_multiplier=133;long_width_multiplier=166; titles=("Gebruikerswachtwoord" "Eigenaarwachtwoord" "Niet-gedefinieerd eigenaarwachtwoord" "Afdruktoestemmingen" "Wijzigingstoestemmingen" "Extractietoestemmingen"); for (( i = 0; i < ${#titles[@]}; i++ )); do title_length=${#titles[i]}; width_var=$((width_default * (title_length >= title_length_threshold ? long_width_multiplier : short_width_multiplier) / 100)); done && file_input=%f && upassword=$(kdialog --geometry ${width_var}x100 --title "${titles[0]}" --password "Voer het gebruikerswachtwoord in.\nDit wachtwoord is nodig om het bestand te openen.\nLaat het veld leeg indien niet nodig." "") && opassword=$(kdialog --geometry ${width_var}x100 --title "${titles[1]}" --newpassword "Voer het eigenaarwachtwoord in.\nHet wachtwoord is vereist." "") && if [ -z "$opassword" ]; then kdialog --geometry ${width_var}x100 --title "${titles[2]}" --error "Eigenaarwachtwoord niet opgegeven. De bewerking wordt geannuleerd."; exit 1; fi && output_file=$(basename "$file_input" | awk -F'.' '{print $1"_gecodeerd"}').pdf && printauth=$(kdialog --geometry ${width_var}x100 --title "${titles[3]}" --radiolist "Selecteer afdruktoestemmingen" "none" "Afdrukken niet toestaan" off "full" "Document afdrukken in hoge resolutie" on "low" "Document afdrukken in lage resolutie" off) && modauth=$(kdialog --geometry ${width_var}x100 --title "${titles[4]}" --radiolist "Selecteer wijzigingstoestemmingen" "none" "Geen wijzigingen toegestaan" on "assembly" "Documentassemblage toestaan" off "form" "Formulieren invullen en ondertekenen toestaan" off "annotate" "Annotatie toestaan" off "all" "Alle wijzigingen toestaan" off) && extractauth=$(kdialog --geometry ${width_var}x100 --title "${titles[5]}" --radiolist "Selecteer extractietoestemmingen" "n" "Niet toegestaan" on "y" "Alles toestaan" off) && qpdf "$file_input" --encrypt "$upassword" "$opassword" 256 --print="$printauth" --modify="$modauth" --extract="$extractauth" -- "$output_file" && kdialog --icon application-pdf --passivepopup "Het PDF-bestand is succesvol gecodeerd!" 10

