Exec[de]=width_default=300;title_length_threshold=20;short_width_multiplier=133;long_width_multiplier=166; titles=("Benutzerpasswort" "Eigentümerpasswort" "Undefiniertes Eigentümerpasswort" "Druckberechtigungen" "Änderungsberechtigungen" "Extraktionsberechtigungen"); for (( i = 0; i < ${#titles[@]}; i++ )); do title_length=${#titles[i]}; width_var=$((width_default * (title_length >= title_length_threshold ? long_width_multiplier : short_width_multiplier) / 100)); done && file_input=%f && upassword=$(kdialog --geometry ${width_var}x100 --title "${titles[0]}" --password "Geben Sie das Benutzerpasswort ein.\nDieses Passwort wird zum Öffnen der Datei benötigt.\nFeld leer lassen, wenn nicht erforderlich." "") && opassword=$(kdialog --geometry ${width_var}x100 --title "${titles[1]}" --newpassword "Geben Sie das Eigentümerpasswort ein.\nDas Passwort ist erforderlich." "") && if [ -z "$opassword" ]; then kdialog --geometry ${width_var}x100 --title "${titles[2]}" --error "Eigentümerpasswort nicht angegeben. Der Vorgang wird abgebrochen."; exit 1; fi && output_file=$(basename "$file_input" | awk -F'.' '{print $1"_verschlüsselt"}').pdf && printauth=$(kdialog --geometry ${width_var}x100 --title "${titles[3]}" --radiolist "Druckberechtigungen auswählen" "none" "Drucken nicht erlauben" off "full" "Dokument in hoher Auflösung drucken" on "low" "Dokument in niedriger Auflösung drucken" off) && modauth=$(kdialog --geometry ${width_var}x100 --title "${titles[4]}" --radiolist "Änderungsberechtigungen auswählen" "none" "Keine Änderungen erlaubt" on "assembly" "Dokumentenmontage erlauben" off "form" "Formulare ausfüllen und unterschreiben erlauben" off "annotate" "Anmerkungen erlauben" off "all" "Alle Änderungen erlauben" off) && extractauth=$(kdialog --geometry ${width_var}x100 --title "${titles[5]}" --radiolist "Extraktionsberechtigungen auswählen" "n" "Nicht erlaubt" on "y" "Alles erlauben" off) && qpdf "$file_input" --encrypt "$upassword" "$opassword" 256 --print="$printauth" --modify="$modauth" --extract="$extractauth" -- "$output_file" && kdialog --icon application-pdf --passivepopup "Das PDF wurde erfolgreich verschlüsselt!" 10





